{
  "account": "123456789000",
  "region": "eu-west-3",
  "serviceName": "my-backend",
  "apexDomain": "my-aws-domain.com",
  "hostedZoneId": "ABCDEFGHIJ1234567890",

  "wantGrafana": false,
  "appPortNum": 8080,
  "terminationWaitTimeMinutes": 5,

  "dev": {
    "stagingEnvironment": "dev",
    "computePlatform": "ecs",
    "imageSource": "dockerhub",
    "repositoryName": "dec1/spring-aws-app",
    "s3BucketName": "mpb-backend-bucket-dev",
    "s3BucketIsCdkManaged": true,
    "imageTag": "1.62.0",
    "healthCheckCommand": [
      "CMD-SHELL",
      "wget --quiet --tries=1 --spider http://localhost:8080/actuator/health || exit 1"
    ],
    "healthCheckPath": "/actuator/health",
    "hostnamePrefix": "dev.api"
  },

  "release": {
    "stagingEnvironment": "release",
    "computePlatform": "ecs",
    "imageSource": "dockerhub",
    "repositoryName": "dec1/spring-aws-app",
    "s3BucketName": "mpb-backend-bucket-release",
    "s3BucketIsCdkManaged": true,
    "imageTag": "1.25.0",
    "healthCheckCommand": [
      "CMD-SHELL",
      "wget --quiet --tries=1 --spider http://localhost:8080/actuator/health || exit 1"
    ],
    "healthCheckPath": "/actuator/health",
    "hostnamePrefix": "api"
  }


}