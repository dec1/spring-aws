{
  "account": "123456789000",
  "serviceName": "my-backend",
  "domainName": "my-aws-domain.com",
  "hostedZoneId": "ABCDEFGHIJ1234567890",


  "wantGrafana": true,
  "appPortNum": 8080,
  "terminationWaitTimeMinutes": 5,

  "dev": {
    "stagingEnvironment": "dev",
    "computePlatform": "ecs",
    "imageSource": "dockerhub",
    "repositoryName": "dec1/spring-aws-app",
    "s3BucketName": "my-backend-bucket-dev",
    "bucketIsCdkManaged": true,
    "imageTag": "1.59.0",
    "healthCheckCommand": [
      "CMD-SHELL",
      "wget --quiet --tries=1 --spider http://localhost:8080/actuator/health || exit 1"
    ],
    "healthCheckPath": "/actuator/health",
    "subdomain": "dev.api"
  },

  "release": {
    "stagingEnvironment": "release",
    "computePlatform": "ecs",
    "imageSource": "dockerhub",
    "repositoryName": "dec1/spring-aws-app",
    "s3BucketName": "my-backend-bucket-release",
    "bucketIsCdkManaged": true,
    "imageTag": "1.25.0",
    "healthCheckCommand": [
      "CMD-SHELL",
      "wget --quiet --tries=1 --spider http://localhost:8080/actuator/health || exit 1"
    ],
    "healthCheckPath": "/actuator/health",
    "subdomain": "api"
  }

}
